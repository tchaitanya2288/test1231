# !/usr/bin/python3
from tkinter import *
from tkinter import messagebox

top = Tk()

top.geometry("200x200")

def helloCallBack():
   msg=messagebox.showinfo( "Home", "Welcome to Python GUI World")


B = Button(top, text ="CLICK TO LOGIN", command = helloCallBack)

B.place(x=0,y=0)

top.mainloop()

