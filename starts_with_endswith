#!/usr/bin/python3
str1 = "this is python world....lets rock!!!"

print(str1,list(enumerate(str1)))

print("")

print (str1.startswith('This'))

print (str1.startswith('python',8,14))

print (str1.startswith('world',15,20))


str2 = "abc mahesh"

suffix = "mahesh"

print(str2,list(enumerate(str2)))

print (str2.endswith(suffix))
print (str2.endswith(suffix,0))

print (str2.endswith(suffix, 0, 3))
print (str2.endswith(suffix, 4, 10))
